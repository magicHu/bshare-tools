<%- model_class = AdFeedback -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:ID) %>:</strong></dt>
  <dd><%= @ad_feedback.ID %></dd>
  <dt><strong><%= model_class.human_attribute_name(:BRANCH_ID) %>:</strong></dt>
  <dd><%= @ad_feedback.BRANCH_ID %></dd>
  <dt><strong><%= model_class.human_attribute_name(:AD_ID) %>:</strong></dt>
  <dd><%= @ad_feedback.AD_ID %></dd>
  <dt><strong><%= model_class.human_attribute_name(:FEED_TIME) %>:</strong></dt>
  <dd><%= @ad_feedback.FEED_TIME %></dd>
  <dt><strong><%= model_class.human_attribute_name(:JOIN_USERID) %>:</strong></dt>
  <dd><%= @ad_feedback.JOIN_USERID %></dd>
  <dt><strong><%= model_class.human_attribute_name(:OTHER_USERID) %>:</strong></dt>
  <dd><%= @ad_feedback.OTHER_USERID %></dd>
  <dt><strong><%= model_class.human_attribute_name(:COMM) %>:</strong></dt>
  <dd><%= @ad_feedback.COMM %></dd>
  <dt><strong><%= model_class.human_attribute_name(:TOTAL_PRICE) %>:</strong></dt>
  <dd><%= @ad_feedback.TOTAL_PRICE %></dd>
  <dt><strong><%= model_class.human_attribute_name(:OUT_BIZNO) %>:</strong></dt>
  <dd><%= @ad_feedback.OUT_BIZNO %></dd>
  <dt><strong><%= model_class.human_attribute_name(:PAY_MODE) %>:</strong></dt>
  <dd><%= @ad_feedback.PAY_MODE %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ORDER_STATUS) %>:</strong></dt>
  <dd><%= @ad_feedback.ORDER_STATUS %></dd>
  <dt><strong><%= model_class.human_attribute_name(:PAY_STATUS) %>:</strong></dt>
  <dd><%= @ad_feedback.PAY_STATUS %></dd>
  <dt><strong><%= model_class.human_attribute_name(:CONFIRMD) %>:</strong></dt>
  <dd><%= @ad_feedback.CONFIRMD %></dd>
  <dt><strong><%= model_class.human_attribute_name(:REMARK) %>:</strong></dt>
  <dd><%= @ad_feedback.REMARK %></dd>
  <dt><strong><%= model_class.human_attribute_name(:INSERT_TIME) %>:</strong></dt>
  <dd><%= @ad_feedback.INSERT_TIME %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              ad_ad_feedbacks_path(@ad), :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_ad_ad_feedback_path(@ad, @ad_feedback), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              ad_ad_feedback_path(@ad, @ad_feedback),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
